{
  "manifest_version": 2,
  "name": "FindYourTab",
  "version": "1.0",
  "description": "Quickly find and switch between browser tabs",
  "permissions": ["tabs"],
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "FindYourTab",
    "default_width": 600
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}